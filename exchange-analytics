#!/usr/bin/env python3
"""
Exchange Analytics CLI Executable
CLI実行可能スクリプト

このスクリプトでCLIアプリケーションを直接実行できます
"""

import sys
from pathlib import Path

# プロジェクトルートをPythonパスに追加
project_root = Path(__file__).parent
sys.path.insert(0, str(project_root))

# CLIメイン関数をインポートして実行
from src.presentation.cli.main import cli_main

if __name__ == "__main__":
    cli_main()
